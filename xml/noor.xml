<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<site name="Guru 99 Bank" url="http://demo.guru99.com">
    <scenario name="Register">
        <form name="Guru 99 bank Register">
            <text value="Enter your email address to get"/>
            <not-text value="Error 404"/>
            <fill id="emailid" value="email@example.com"/>
            <submit element="emailid"/>
            <text value="Access details to demo site."/>
            <text value="This access is valid only for 20 days."/>
            <store key="username" id="/html/body/table/tbody/tr[4]/td[2]"/>
            <store key="password" id="/html/body/table/tbody/tr[5]/td[2]"/>
            <not-text value="Error 404"/>
        </form>
    </scenario>
    <scenario name="Login">
        <form name="Login Form" url="/V4/index.php">
            <text value="Steps To Generate Access"/>
            <fill id="uid"/>
            <fill id="password"/>
            <submit element="uid"/>
            <text value="Welcome To Manager's Page of Guru99 Bank"/>
        </form>
    </scenario>
</site>
