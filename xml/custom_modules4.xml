<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<site name="Google" url="https://www.google.jo/">
    <scenario name="Custom Code Injection! - Test  #4 - Multiple Methods">
        <form name="Search Page" url="">
            <text value="الأردن"/>
            <custom title="FourthModularCustomCode">
                <var type="String" name="url" value="http://www.abukhleif.com/"/>
                <var type="String" name="text" value="Latest stories"/>
                <var type="String" name="notText" value="Page not found"/>
                <var type="String" name="value" value="Hello World"/>
                <method name="navigateAndVerify">
                    <code>navigate(url);
                        verify(text, true);
                        verify(notText, false);
                    </code>
                </method>
            </custom>
        </form>
    </scenario>
</site>
